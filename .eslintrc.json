{
  "env": {
    "browser": true,
    "es2021": true,
    "es6": true,
    "node": true,
    "jest": true  // Убедитесь, что это добавлено
  },
  "extends": ["eslint:recommended"],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "semi": ["error", "always"],
    "no-console": "off",
    "import/extensions": "off",
    "no-unused-vars": "off"
  }
}
